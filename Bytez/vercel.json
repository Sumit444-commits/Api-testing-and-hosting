{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/bytez/(.*)",
      "destination": "https://api.bytez.com/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}